{
  "name": "ati",
  "main": "./src/index.expo.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "yarn build:report && yarn flow:report",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "flow": "flow",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:coverage": "flow coverage",
    "flow:report":
      "flow-coverage-report -i 'src/**/*.js' -x 'src/test/**' -x 'src/**/*.test.js' -x 'src/**/*.spec.js' -t html -t json -t text --threshold 90 -o ./reports/flow/",
    "build": "react-scripts build",
    "build:report":
      "source-map-explorer  --html ./build/static/js/main.* > ./reports/bundle-report.html",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "eject:cra-dont-do": "react-scripts eject",
    "cz": "git-cz",
    "appr": "appr",
    "terraform": "cd ./terrafuzz && sudo ./terrafuzz.sh apply && cd .."
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chroma-js": "^1.3.4",
    "es6-symbol": "^3.1.1",
    "expo": "^26.0.0",
    "final-form": "^4.4.0",
    "history": "^4.7.2",
    "query-string": "5",
    "raven-js": "^3.24.0",
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-final-form": "^3.1.5",
    "react-native":
      "https://github.com/expo/react-native/archive/sdk-26.0.0.tar.gz",
    "react-native-web": "^0.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-first-router": "^0.0.20-next",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "svgs": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "appr": "^2.0.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "23.0.0-alpha.4",
    "commitizen": "^2.9.6",
    "cypress": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-cypress-dev": "^1.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "exp": "^53.0.0",
    "flow-bin": "0.63.0",
    "flow-coverage-report": "^0.5.0",
    "husky": "0.15.0-rc.9",
    "prettier": "^1.11.1",
    "react-test-renderer": "^16.3.1",
    "source-map-explorer": "^1.5.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "precommit": "yarn lint"
    }
  }
}
