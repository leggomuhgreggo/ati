{
  "name": "ati",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "yarn build:report && yarn flow:report",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "flow": "flow",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:coverage": "flow coverage",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -x 'src/test/**' -x 'src/**/*.test.js' -x 'src/**/*.spec.js' -t html -t json -t text --threshold 90 -o ./reports/flow/",
    "build": "react-scripts build",
    "eject:cra-dont-do": "react-scripts eject",
    "terraform": "cd ./terrafuzz && sudo ./terrafuzz.sh apply && cd .."
  },
  "dependencies": {
    "nuka-carousel": "^4.3.4",
    "react": "^16.4.1",
    "react-art": "^16.4.0",
    "react-dom": "^16.4.1",
    "react-icons": "^3.0.5",
    "react-native-web": "^0.8.8",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.0",
    "casual-browserify": "^1.5.19-2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.80.0",
    "prettier": "^1.13.5",
    "react-styleguidist": "^7.1.0",
    "react-test-renderer": "^16.4.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
